{
  "version": "1.2.0-*",
  "buildOptions": {
    "emitEntryPoint": true,
    "preserveCompilationContext": true,
    "copyToOutput": {
      "include": [
        "Views",
        "wwwroot"
      ]
    }
  },
  "publishOptions": {
    "include": [
      "Views",
      "wwwroot"
    ]
  },
  "frameworks": {
    "netcoreapp1.1": {
      "imports": [
        "dnxcore50",
        "portable-net451+win8"
      ],
      "dependencies": {
        "Microsoft.NETCore.App": {
          "version": "1.2.0-*",
          "type": "platform"
        }
      }
    },
    "net451": {}
  },
  "dependencies": {
    "Microsoft.AspNetCore.Mvc.Core": "1.2.0-*",
    "Microsoft.AspNetCore.Server.IISIntegration": "1.2.0-*",
    "Microsoft.AspNetCore.Server.Kestrel": "1.2.0-*",
    "Microsoft.Extensions.Configuration.CommandLine": "1.2.0-*",
    "Microsoft.Extensions.Configuration.EnvironmentVariables": "1.2.0-*",
    "Microsoft.Extensions.Configuration.Json": "1.2.0-*"
  },
  "commands": {
    "web": "HelloWorldMvcCore"
  },
  "runtimeOptions": {
    "configProperties": {
      "System.GC.Server": true
    }
  }
}