{
  "version": "1.2.0-*",
  "buildOptions": {
    "emitEntryPoint": true,
    "compile": {
      "include": [ "../Shared/**/*.cs" ]
    }
  },
  "dependencies": {
    "Microsoft.Extensions.CommandLineUtils": "1.2.0-*",
    "Newtonsoft.Json": "9.0.1"
  },
  "frameworks": {
    "netcoreapp1.1": {
      "dependencies": {
        "Microsoft.NETCore.App": {
          "type": "platform",
          "version": "1.2.0-*"
        }
      }
    },
    "net451": {
      "frameworkAssemblies": {
        "System.Net.Http": ""
      }
    }
  },
  "runtimeOptions": {
    "configProperties": {
      "System.GC.Server": true
    }
  }
}
